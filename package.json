  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format:fix": "prettier --write \"**/*.{tsx}\"",
    "format": "prettier --check \"**/*.{tsx}\"",
