    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:eslint": "eslint '**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "eslint --fix '**/*.{js,jsx,ts,tsx}'",
    "format": "prettier --check '**/*.{js,jsx,ts,tsx}'",
